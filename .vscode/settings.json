{
    "files.exclude": {
        "**/node_modules": true,
        "**/compiled": true
    },
    "editor.tabSize": 4,
    "editor.autoIndent": "full",
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "files.eol": "\n",
    "javascript.suggestionActions.enabled": false
}
